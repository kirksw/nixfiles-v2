  "runtime.version": "LuaJIT",
  "runtime.path": ["?.lua", "?/?.lua"],
  "diagnostics.globals": ["vim", "LazyVim", "Snacks"],
  "workspace.library": ["$VIMRUNTIME"],
  "workspace.checkThirdParty": false,
